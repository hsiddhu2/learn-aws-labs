{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "IAM user policy",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::*"
            ]
        },
        {
            "Sid": "Stmt1420751757001",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource":[
                "arn:aws:s3:::mybucket/*"
            ]
        }
    ]
}